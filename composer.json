{
    "name": "theposeidonas/laravel-emoji-parser",
    "description": "A Laravel package for handling emoji expressions in blade files.",
    "keywords": ["laravel", "emoji", "parser"],
    "license": "MIT",
    "version": "v1.0.0",
    "authors": [
        {
            "name": "Baran Arda",
            "email": "info@baranarda.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Theposeidonas\\LaravelEmojiParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Theposeidonas\\LaravelEmojiParser\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Theposeidonas\\LaravelEmojiParser\\EmojiServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
